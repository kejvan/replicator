  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5c6370"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "String Punctuation",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end",
        "string.quoted.single punctuation.definition.string",
        "string.quoted.double punctuation.definition.string",
        "string.quoted.triple punctuation.definition.string",
        "string.template punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "String Escape",
      "scope": [
        "constant.character.escape",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "Keywords Control",
      "scope": [
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.function",
        "support.class",
        "support.type",
        "support.constant"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "meta.class",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Method Definition",
      "scope": [
        "entity.name.method",
        "entity.name.function.method"
      ],
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Variables: Other Property",
      "scope": [
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.comparison",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic"
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#abb2bf"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
